{
    "board": "rm01_esp32s3",
    "version": "1.0",
    "description": "Pin mapping for RM01 ESP32S3 board",
    "pins": {
        "LED_TOUCH": {
            "gpio": 45,
            "description": "Touch LED WS2812 data line"
        },
        "LED_BOARD": {
            "gpio": 42,
            "description": "Board LED WS2812 data line"
        },
        "LED_MATRIX": {
            "gpio": 9,
            "description": "Matrix LED WS2812 data line"
        },
        "FAN_PWM_0": {
            "gpio": 41,
            "description": "Fan PWM control (25kHz, 10-bit resolution)"
        },
        "ETH_MISO": {
            "gpio": 13,
            "description": "W5500 SPI MISO"
        },
        "ETH_MOSI": {
            "gpio": 11,
            "description": "W5500 SPI MOSI"
        },
        "ETH_SCLK": {
            "gpio": 12,
            "description": "W5500 SPI SCLK"
        },
        "ETH_CS": {
            "gpio": 10,
            "description": "W5500 SPI CS"
        },
        "ETH_INT": {
            "gpio": 38,
            "description": "W5500 interrupt"
        },
        "ETH_RST": {
            "gpio": 39,
            "active_low": true,
            "initial_state": "high",
            "description": "W5500 reset (LOW=reset, HIGH=normal)"
        },
        "USB_MUX_0": {
            "gpio": 8,
            "description": "USB MUX0 select (ESP32_MUX0_SEL)"
        },
        "USB_MUX_1": {
            "gpio": 48,
            "description": "USB MUX1 select (ESP32_MUX1_SEL)"
        },
        "POWER_ADC": {
            "gpio": 18,
            "description": "DC input voltage monitor (ADC2_CH7, divider 11.4:1, max 72V)"
        },
        "POWER_UART_RX": {
            "gpio": 47,
            "description": "Power chip data RX (UART1_RX, 9600 8N1, [0xFF][V][I][CRC])"
        },
        "AGX_FORCE_SHUTDOWN": {
            "gpio": 3,
            "active_low": true,
            "initial_state": "high",
            "description": "AGX force shutdown (LOW=force off, HIGH=normal)"
        },
        "AGX_RESET": {
            "gpio": 1,
            "active_high": true,
            "initial_state": "low",
            "pulse_ms": 1000,
            "description": "AGX reset control (HIGH=reset, LOW=normal, pulse 1000ms)"
        },
        "AGX_RECOVERY": {
            "gpio": 40,
            "active_high": true,
            "initial_state": "low",
            "description": "AGX recovery mode (HIGH=enable recovery, LOW=normal)"
        },
        "LPMU_POWER": {
            "gpio": 46,
            "active_high": true,
            "initial_state": "low",
            "pulse_ms": 300,
            "description": "LPMU power button (pulse HIGH 300ms to toggle)"
        },
        "LPMU_RESET": {
            "gpio": 2,
            "active_high": true,
            "initial_state": "low",
            "pulse_ms": 300,
            "description": "LPMU reset (pulse HIGH 300ms to reset)"
        },
        "RTL8367_RST": {
            "gpio": 17,
            "active_high": true,
            "initial_state": "low",
            "description": "RTL8367 switch reset (HIGH=reset, LOW=normal)"
        },
        "SD_CMD": {
            "gpio": 15,
            "description": "SD card CMD"
        },
        "SD_CLK": {
            "gpio": 16,
            "description": "SD card CLK (40MHz)"
        },
        "SD_D0": {
            "gpio": 4,
            "description": "SD card D0"
        },
        "SD_D1": {
            "gpio": 5,
            "description": "SD card D1"
        },
        "SD_D2": {
            "gpio": 6,
            "description": "SD card D2"
        },
        "SD_D3": {
            "gpio": 7,
            "description": "SD card D3"
        }
    }
}
